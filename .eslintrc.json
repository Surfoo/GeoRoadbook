{
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-console": ["off"]
    },
    "env": {
        "es6": true,
        "browser": true,
        "jquery": true
    },
    "globals": {
        "user": false,
        "tinyMCE": false,
        "tinymce": false,
        "language": false,
        "roadbook_id": false,
        "ActiveXObject": false
    },
    "extends": "eslint:recommended"
}
